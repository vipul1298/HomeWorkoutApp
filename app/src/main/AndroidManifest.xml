<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.workout">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RandomActivity" android:parentActivityName=".RandomList"></activity>
        <activity android:name=".RandomList" android:parentActivityName=".MainActivity" />
        <activity android:name=".ExerciseActivity" android:parentActivityName=".DayWise" />
        <activity
            android:name=".DayWise"
            android:parentActivityName=".DayList" />
        <activity
            android:name=".Developer"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".DayList"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>